<?xml version="1.0"?>
<launch>
<!--node-->

<!--manager-->
    <node pkg="nodelet" type="nodelet" name="manager" args="manager" output="screen"/>
<!--nodelet-->
<!--    <node pkg="nodelet" type="nodelet" name="prm_localization_nodelet" args="load prm_localization/PRMLocalizationNodelet manager"/>-->
    <node pkg="nodelet" type="nodelet" name="rt_localization_nodelet" args="load prm_localization/GlobalmapProviderNodelet manager">
        <param name="trim_low" value="0.0f" />
        <param name="trim_high" value="4.0f" />
        <param name="init_x" value="0.0f" />
        <param name="init_y" value="0.0f" />
        <param name="init_raw" value="0.0" />
        <param name="icpMaximumIterations" value="50" />
        <param name="icpRANSACOutlierRejectionThreshold" value="0.04f" />
        <param name="icpTransformationEpsilon" value="1e-7" />
        <param name="downSampleSize" value="0.05f" />
    </node>
    <node pkg="nodelet" type="nodelet" name="globalmap_provider_nodelet" args="load prm_localization/RealTime_Localization manager">
        <param name="downsample_resolution" value="0.05f" />
        <param name="radius" value="40.0f" />
    </node>


</launch>
